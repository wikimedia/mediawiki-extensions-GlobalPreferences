		"PreferencesFormPreSave": [
			"GlobalPreferences\\Hooks::onPreferencesFormPreSave"
		],
		"LoadExtensionSchemaUpdates": [
			"GlobalPreferences\\Hooks::onLoadExtensionSchemaUpdates"
